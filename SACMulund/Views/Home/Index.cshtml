</section>

<section>

    <div class="row">
        <div class="col-md-3 contact-form-style">
            <div class="padding-13">
                <span id='close' style="float:right;" onclick='this.parentNode.parentNode.parentNode.removeChild(this.parentNode.parentNode); return false;'>x</span>
                <h2>Inquiry</h2>
                <div id="formsendmail">
                    <div class="form-group">
                        <label class="label-color" for="name">Full Name<span style="color:red">*</span>:</label>
                        <input type="text" class="form-control" id="name" placeholder="Enter email" name="email" data-error="Name is Required" required />
                    </div>
                    <div class="form-group">
                        <label class="label-color" for="email">Email Id<span style="color:red">*</span>: </label>
                        <input type="email" class="form-control" id="email" placeholder="Enter Email" name="pswd" required />
                    </div>
                    <div class="form-group">
                        <label class="label-color" for="Number">Mobile Number<span style="color:red">*</span>:</label>
                        <input type="text" class="form-control" id="number" placeholder="Enter Number" name="pswd" required />
                    </div>
                    <button type="button" class="col-md-12 btn btn-primary" onclick="sendmail(); return false;">Submit</button>
                </div>
                <div id="successmessage" class="font-size-25" style="display:none;">
                    Thank you for showing your interest! Our sales team will get in touch with you shortly!
                </div>
            </div>
        </div>
    </div>

</section>
@section Scripts {
    <script type="text/javascript">
        function sendmail() {
            $.post("/home/sendmail", { name: $("#name").val(), email: $("#email").val(), number: $("#number").val() })
                .done(function (json) {
                    console.log(json);
                    // mail sent
                    $("#name").val('');
                    $("#email").val('');
                    $("#number").val('');

                    $("#successmessage").show();
                    $("#formsendmail").hide();
                })
                .fail(function (jqxhr, textStatus, error) {
                    // sending mail error
                });
        }
    </script>
}
<script>
    window.onload = function () {
        document.getElementById('close').onclick = function () {
            this.parentNode.parentNode.parentNode
                .removeChild(this.parentNode.parentNode);
            return false;
        };
    };
</script>

